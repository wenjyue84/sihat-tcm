{
  "name": "sihat-tcm-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev -p 3100",
    "dev-https": "next dev -H 0.0.0.0 -p 3100 --experimental-https",
    "dev:tina": "tinacms dev -c \"next dev -p 3100\"",
    "dev:turbo": "next dev -p 3100 --turbo",
    "dev:clear": "rm -rf .next && npm run dev",
    "build": "next build",
    "build:tina": "tinacms build && next build",
    "start": "next start -p 3100",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:pbt": "vitest --run src/lib/testing",
    "test:property": "vitest --run src/lib/testing",
    "test:pbt-report": "node scripts/run-property-tests.js",
    "test:coverage": "vitest --coverage",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "rollback:staging": "bash scripts/rollback.sh staging",
    "rollback:production": "bash scripts/rollback.sh production",
    "monitor:staging": "bash scripts/monitor-deployment.sh staging",
    "monitor:production": "bash scripts/monitor-deployment.sh production",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "node -e \"if (!process.env.CI) try { require('child_process').execSync('husky', {stdio: 'inherit'}) } catch (e) {}\""
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tinacms/cli": "^2.0.5",
    "ai": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.26",
    "fs-extra": "^11.3.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.555.0",
    "lunar-javascript": "^1.7.7",
    "next": "16.1.1",
    "next-mdx-remote": "^5.0.0",
    "next-swagger-doc": "^0.4.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "19.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "reading-time": "^1.5.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
    "slate": "^0.120.0",
    "slate-dom": "^0.119.0",
    "slate-react": "^0.120.0",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "tinacms": "^3.1.2",
    "use-debounce": "^10.0.6",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "fast-check": "^4.5.2",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "jsonpath-plus": "10.3.0",
    "dompurify": "3.2.4",
    "@tinacms/metrics": {
      "fs-extra": "$fs-extra"
    },
    "tinacms": {
      "@headlessui/react": {
        "react": "$react",
        "react-dom": "$react-dom"
      },
      "react-final-form": {
        "react": "$react"
      }
    }
  }
}